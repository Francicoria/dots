# .mkshrc

# If not running interactively, don't do anything
[[ $- != *i* ]] && return 0
# Autostart

# Aliases
alias ls='ls -lAh --color=auto'
alias rm='rm -i'
alias cp='cp -i'
alias nnn='nnn -C -H'
alias less='nvimpager'

alias xpbs-install='xbps-install'
alias xpbs-remove='xbps-remove'
alias xpbs-query='xbps-query'

bind -m '^L'=clear'^J'

# PS1
PS1='[${PWD/#"$HOME"/\~}] \$ '
